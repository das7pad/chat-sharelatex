{
  "name": "chat-sharelatex",
  "version": "0.1.4",
  "description": "The backend API that powers ShareLaTeX chat",
  "repository": {
    "type": "git",
    "url": "https://github.com/sharelatex/chat-sharelatex.git"
  },
  "scripts": {
    "start": "node $NODE_APP_OPTIONS app.js",
    "test:acceptance": "npm run test:acceptance:_run -- --grep=$MOCHA_GREP",
    "test:unit": "npm run test:unit:_run -- --grep=$MOCHA_GREP",
    "nodemon": "nodemon --config nodemon.json",
    "test:acceptance:_run": "mocha --recursive --reporter spec --timeout 15000 --exit $@ test/acceptance/js",
    "test:unit:_run": "mocha --recursive --reporter spec $@ test/unit/js",
    "lint": "make lint",
    "format": "make format",
    "format:fix": "make format_fix"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.12",
    "express": "^4.17.1",
    "logger-sharelatex": "git+https://github.com/das7pad/logger-sharelatex.git#273abe254b90cad45ab9b04436e50eedb3981053",
    "metrics-sharelatex": "git+https://github.com/das7pad/metrics-sharelatex.git#b6b360e7adb8f6b4ccd8deab8a70ec95cb24840f",
    "mongojs": "git+https://github.com/das7pad/mongojs.git#e39e97bf289435e2d111c6201d3306b6b776d31e",
    "settings-sharelatex": "^1.1.0"
  },
  "devDependencies": {
    "async": "^3.2.0",
    "chai": "^4.2.0",
    "mocha": "^7.1.0",
    "request": "^2.88.2",
    "sandboxed-module": "^2.0.3",
    "sinon": "^9.0.0"
  }
}
